<% if !equipo.nil?%>
 <tr>
  <td><%= equipo.nombre %></td>
  <td><%= equipo.descripcionEs %></td>
  <td><%= equipo.descripcionFr.force_encoding('UTF-8') %></td>
  <td><%= link_to t('show'), sitio_path(equipo.sitio)%></td> 
  <td><%= link_to t('edit'), edit_equipo_path(equipo)%></td> 
  <td><%= link_to t('destroy'), equipo, :confirm => t('confirm'),  :method => :delete %></td>
 </tr>
<%end%>
