 <tr>
    <td><%= colaborador.nombreEs %></td>
    <td><%= colaborador.nombreFr %></td>
    <td><%= colaborador.url %></td>
    <td> <% if colaborador.imagen_url.to_s != "" %>
    <%= image_tag colaborador.imagen_url.to_s %>
    <% end %></td>
   <td><%= link_to t('show'), colaborador_path(colaborador)%></td>
   <td><%= link_to t('edit'), edit_colaborador_path(colaborador)%></td>
   <td><%= link_to t('destroy'), colaborador, :confirm =>t('confirm'),  :method => :delete %></td>
 </tr>
