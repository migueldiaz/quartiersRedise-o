<%if colaboradorSi.aporta==true%>
 <tr>
   <td><%= colaboradorSi.nombreEs %></td>
    <td><%= colaboradorSi.nombreFr %></td>
     <td><%= colaboradorSi.url %></td>
   <td> <% if colaboradorSi.imagen_url.to_s != "" %>
    <%= image_tag colaboradorSi.imagen_url.to_s,:size=>'80x70' %>
    <% end %></td>
   <td><%= link_to t('show'), colaborador_path(colaboradorSi)%></td>
   <td><%= link_to t('edit'), edit_colaborador_path(colaboradorSi)%></td>
    <% if !is_traductor %>
   <td><%= link_to t('destroy'), colaboradorSi, :confirm => t('confirm'),  :method => :delete %></td>
   <%end%>
 </tr>
<%end%>
